ifneq ($(KERNELRELEASE),)

# kbuild part of makefile
obj-m  := sv_driver.o
sv_driver-y := sv_driver_bin.o 

else
#  normal makefile
KDIR ?= /lib/modules/`uname -r`/build

default:
	$(MAKE) -C $(KDIR) M=$$PWD

clean:
	make -C /lib/modules/$(shell uname -r)/build M=$(PWD) clean


# Module specific targets
genbin:
	echo "X" > sv_driver_bin.o_shipped

endif
