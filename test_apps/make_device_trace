insmod vsi_driver.ko cdma_address="0x40001000" cdma_2_address="0x40002000" device_id="0x7028" int_ctlr_address="0x40000000" pcie_ctl_address="0x40003000" pcie_m_address="0x00000000" enable_cdma_2="1"

rm -rf /dev/vsi_system_vc709_packet_p_arg_2_seq_o
rm -rf /dev/vsi_system_vc709_packet_p_arg_1_seq_i
rm -rf /dev/trace_read
rm -rf /dev/trace_control

mknod /dev/vsi_system_vc709_packet_p_arg_2_seq_o c 241 1
chown root /dev/vsi_system_vc709_packet_p_arg_2_seq_o
chmod 666  /dev/vsi_system_vc709_packet_p_arg_2_seq_o

mknod /dev/vsi_system_vc709_packet_p_arg_1_seq_i c 241 2
chown root /dev/vsi_system_vc709_packet_p_arg_1_seq_i
chmod 666  /dev/vsi_system_vc709_packet_p_arg_1_seq_i

mknod /dev/trace_read c 241 3
chown root /dev/trace_read
chmod 666  /dev/trace_read

mknod /dev/trace_control c 241 4
chown root /dev/trace_control
chmod 666  /dev/trace_control
